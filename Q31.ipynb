    "def calculate():\n",
    "    TOTAL = 200\n",
    "    ways = [1] + [0] * TOTAL\n",
    "    for coin in [1, 2, 5, 10, 20, 50, 100, 200]:\n",
    "        for i in range(len(ways) - coin):\n",
    "            ways[i + coin] += ways[i]\n",
    "    return str(ways[-1])\n",
    "\n",
    "print(calculate())\n"
   ]
