    "def get_first_number_having_n_digits(n):\n",
    "    i = 0\n",
    "    j = 1\n",
    "    term = 1\n",
    "\n",
    "    while (len(str(j)) < (n)):\n",
    "        k = j + i\n",
    "        i = j\n",
    "        j = k\n",
    "        term += 1\n",
    "    return term\n",
    "\n",
    "res = get_first_number_having_n_digits(1000)\n",
    "print(res)"
   ]
